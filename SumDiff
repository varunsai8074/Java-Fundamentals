import java.util.*;
public class SumDiff
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int sum = a+b;
        int dif = a-b;
        System.out.println(sum);
        System.out.println(dif);
    }
}
